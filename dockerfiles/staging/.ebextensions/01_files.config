files:
    "/etc/nginx/conf.d/gzip.conf" :
        mode: "000755"
        owner: root
        group: root
        content: |
          gzip on;
          gzip_proxied any;
          gzip_types text/plain text/xml text/html text/css application/x-javascript application/json text/javascript;
          gzip_vary on;
          gzip_disable "MSIE [1-6]\.(?!.*SV1)";
    "/etc/nginx/conf.d/proxy.conf" :
        mode: "000755"
        owner: root
        group: root
        content: |
           client_max_body_size 50M;
